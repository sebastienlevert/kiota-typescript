{
	"compilerOptions": {
		"importHelpers": true,
		"noEmitOnError": false,
		"moduleResolution": "node",
		"strict": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictFunctionTypes": true,
        "strictNullChecks": true,
        "forceConsistentCasingInFileNames": true,
		"declaration": true,
        "composite": true
	}
}